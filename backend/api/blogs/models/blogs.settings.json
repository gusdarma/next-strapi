{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "navigation": {
    "model": "navigationitem",
    "plugin": "navigation",
    "via": "related",
    "configurable": false,
    "hidden": true
  },
  "attributes": {
    "navigation": {
      "model": "navigationitem",
      "plugin": "navigation",
      "via": "related",
      "configurable": false,
      "hidden": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["published", "draft"],
      "default": "draft",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^$|^[a-zA-Z/-]+$"
    },
    "contentSections": {
      "type": "dynamiczone",
      "components": ["sections.hero-blogs"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "metaDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "shareImage": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
